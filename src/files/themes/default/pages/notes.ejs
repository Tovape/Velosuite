<div class="page-container container">
	<div class="header">
		<h1>Notes</h1>
		<div>
			<%- 
			include(`../components/button.ejs`, { context: "New", className: "button button-1", action: "openPagePopup('notes')" } ) 
			-%>
			<%- 
			include(`../components/selection.ejs`, { list: ["All", "vent"], className: "selection selection-1 note-filter", name: "note-active" } ) 
			-%>
		</div>
	</div>
	<div class="content" id="notes-deposit">
	</div>
</div>
<div class="overlay">
	<div class="content">
		<div class="content-header">
			<p class="title">Create Note</p>
			<img onclick="closePagePopup('notes')" src="../files/themes/<%= theme_name %>/icons/global/close.svg">
		</div>
		<div class="content-main">
			<%- 
			include(`../components/input.ejs`, { placeholder: "Title", className: "input input-1", name: "note-title" } ) 
			-%>
			<%- 
			include(`../components/button.ejs`, { context: "Create", className: "button button-1", action: "newNote()" } ) 
			-%>
		</div>
	</div>
	<div onclick="closePagePopup('notes')" class="background">&nbsp;</div>
</div>	