<meta charset="UTF-8"/>
<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no">
<!-- Fonts -->
<%- 
include(`../../files/themes/${theme_name}/components/fonts.ejs`)
%>
<!-- Android  -->
<meta name="theme-color" content="#6D94DB">
<meta name="mobile-web-app-capable" content="yes">
<!-- iOS -->
<meta name="apple-mobile-web-app-title" content="Velosuite">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="default">
<!-- Windows  -->
<meta name="msapplication-navbutton-color" content="#6D94DB">
<meta name="msapplication-TileColor" content="#6D94DB">
<meta name="msapplication-TileImage" content="../files/images/logo/logo-white-180.png">
<!-- Icon -->
<link rel="icon" href="../files/images/logo/logo-icon.svg" type="image/gif" sizes="16x16">
<link rel="apple-touch-icon" href="../files/images/logo/logo-white-180.png">
<!-- CSS -->
<link rel="stylesheet" type="text/css" href="../css/global.css">
<link rel="stylesheet" type="text/css" href="../files/themes/<%= theme_name %>/css/global.css">
<link rel="stylesheet" type="text/css" href="../files/themes/<%= theme_name %>/components/button.css">
<link rel="stylesheet" type="text/css" href="../files/themes/<%= theme_name %>/components/input.css">
<link rel="stylesheet" type="text/css" href="../files/themes/<%= theme_name %>/components/dropdown.css">
<link rel="stylesheet" type="text/css" href="../files/themes/<%= theme_name %>/components/progress.css">
<link rel="stylesheet" type="text/css" href="../files/themes/<%= theme_name %>/components/popupmessage.css">
<link rel="stylesheet" type="text/css" href="../files/themes/<%= theme_name %>/components/navigator.css">
<link rel="stylesheet" type="text/css" href="../files/themes/<%= theme_name %>/components/weather.css">
<link rel="stylesheet" type="text/css" href="../files/themes/<%= theme_name %>/components/clock.css">
<link rel="stylesheet" type="text/css" href="../files/themes/<%= theme_name %>/components/selection.css">
<!-- JS -->
<script type="text/javascript" src="../js/global.js"></script>
<!-- PWA -->
<link rel="manifest" href="../pwa/pwa-manifest.json">
<script> if ("serviceWorker" in navigator) { navigator.serviceWorker.register("../pwa/pwa-sw.js") }</script>